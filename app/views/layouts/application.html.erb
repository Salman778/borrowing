<!DOCTYPE html>
  <html>
    <head>
      <title>Borrowing</title>
      <%= csrf_meta_tags %>
      <%= csp_meta_tag %>

      <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
      <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    </head>

    <body>
    <% if signed_in? %>
      <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <b>
          <a class="navbar-brand"><%= link_to 'Home', root_path %></a>
          <a class="navbar-brand"><%= link_to 'My Items', items_path %></a>
          <a class="navbar-brand"><%= link_to 'My Account', edit_user_registration_path %></a>
          <a class="navbar-brand"><%= link_to 'Sign Out', destroy_user_session_path, method: :delete %></a>
        </b>
      </nav>
      <h3> Welcome <%= current_user.name%> </h3>
      <h5>Zip Code: <%= current_user.zip_code%> </h5>
    <% end %>
    <br>
      <p class="notice"><%= notice %></p>
      <p class="alert"><%= alert %></p>
      <%= yield %>
      
    </body>
  </html>
